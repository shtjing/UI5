{
	"_version": "1.7.0",
	"sap.app": {
		"id": "PortalProduct",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_platinum"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "PortalProduct.view.MAIN",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {
					"minVersion": ""
				},
				"sap.m": {
					"minVersion": ""
				},
				"sap.ui.comp": {
					"minVersion": ""
				},
				"sap.ui.core": {
					"minVersion": ""
				},
				"sap.ui.layout": {
					"minVersion": ""
				},
				"sap.ushell": {
					"minVersion": ""
				},
				"sap.uxap": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "PortalProduct.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/yk_port_prd_001/webapp",
		"_version": "1.1.0"
	}
}